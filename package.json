{
  "name": "sy-plugin-enhance",
  "type": "module",
  "version": "0.46.0",
  "description": "This is a plugin based on vite and vue for Siyuan (https://b3log.org/siyuan)",
  "author": "",
  "license": "MIT",
  "homepage": "",
  "repository": "",
  "scripts": {
    "make-link": "node  --no-warnings ./scripts/make_dev_link.js",
    "clear-normalize": "echo > node_modules/@arco-design/web-vue/es/style/normalize.less && echo > node_modules/@arco-design/web-vue/lib/style/normalize.less",
    "dev": "npm run clear-normalize && vite build --watch",
    "release": "node ./release.js",
    "release:manual": "node ./release.js --mode=manual",
    "release:patch": "node ./release.js --mode=patch",
    "release:minor": "node ./release.js --mode=minor",
    "release:major": "node ./release.js --mode=major",
    "build": "npm run clear-normalize && vite build"
  },
  "dependencies": {
    "@arco-design/web-vue": "^2.53.3",
    "@vue-flow/additional-components": "^1.3.3",
    "@vue-flow/background": "^1.3.2",
    "@vue-flow/controls": "^1.1.2",
    "@vue-flow/core": "^1.42.1",
    "@vue-flow/minimap": "^1.5.2",
    "@vue-flow/node-resizer": "^1.4.0",
    "@vue-flow/node-toolbar": "^1.1.0",
    "@vueuse/core": "^13.2.0",
    "any-touch": "2.2.0",
    "chalk": "^5.3.0",
    "dayjs": "^1.11.10",
    "lodash-es": "^4.17.21",
    "mitt": "^3.0.1",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.8.1",
    "@arco-plugins/vite-vue": "^1.4.5",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.13.10",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.22.0",
    "eslint-plugin-format": "^1.0.1",
    "eslint-plugin-perfectionist": "^4.2.0",
    "fast-glob": "^3.2.12",
    "glob": "^11.0.1",
    "less": "^4.2.0",
    "minimist": "^1.2.8",
    "rollup-plugin-livereload": "^2.0.5",
    "sass": "^1.62.1",
    "siyuan": "1.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "unplugin-vue-components": "^28.5.0",
    "vite": "^6.2.1",
    "vite-plugin-static-copy": "^2.3.0",
    "vite-plugin-zip-pack": "^1.0.5"
  }
}
